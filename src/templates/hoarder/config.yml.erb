backend: "file:///var/db/hoarder"    # the pluggable backend the api will use for storage
listen-addr: "https://0.0.0.0:7410"  # the connection host
log-level: "info"                    # the output log level
token: "<%= config[:token] %>"       # the secure token used to connect with (no auth by default)
server: true                         # run as a server
